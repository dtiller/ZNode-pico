set(HEADERS)

include(${CMAKEINC}/util_include.cmake)
include(${CMAKEINC}/port_include.cmake)

cmake_print_variables(HEADERS)
cmake_print_variables(CMAKE_CURRENT_LIST_DIR)

add_executable(test main.c ${SOURCES} ${HEADERS})
target_sources(test PRIVATE ${PORTDIR}/ports_pico.c ${SAMPLEDIR}/${PORT}/sam_pico.c ${SAMPLEDIR}/common/sam_common.c)

target_include_directories(test PRIVATE ${INCDIR}/util ${INCDIR}/wpan ${INCDIR}/xbee ${INCDIR}/zigbee ${PORTDIR} ${SAMPLEDIR}/common ${SAMPLEDIR}/${PORT})

pico_enable_stdio_usb(test 1)
pico_enable_stdio_uart(test 1)
pico_add_extra_outputs(test)
target_link_libraries(test pico_stdlib)
